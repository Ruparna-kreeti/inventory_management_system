<div class="form">
  <div class=" jumbotron">
    <%= form_with model: @employees_item do |f| %>
      <%= render 'shared/errors', object: f.object %>
      <%= f.label :employee_id,"Select employee email: " %><span class="required">*</span>
      <% options = options_from_collection_for_select(@employees, 'id', 'email') %>
      <%= f.select :employee_id,  options %><br/><br/>
      <%= f.label :item_id,"Select Item: " %><span class="required">*</span>
      <% options = options_from_collection_for_select(@items, 'id','item_fullname') %>
      <%= f.select :item_id,  options %><br/><br/>
      <%= f.label :status,"Status: " %><span class="required">*</span>
      <%= f.select :status,[['working',true], ['not working',false]], class:"form-control mt-10" %><br/><br/>
      <%= f.submit yield(:button_text), class:"btn btn-primary" %>
    <% end %>
  </div>
</div>